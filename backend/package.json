{
  "name": "nabin-api",
  "version": "0.0.1",
  "description": "NABIN API that will expose NABIN endpoints for crud operations",
  "author": "Bennett Benedict",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nodemon ts-node nest start",
    "start:dev": "nodemon --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "watch-server": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' ./src/server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "typeorm migration:generate -n",
    "migration:create": "typeorm migration:create -n",
    "migration:run": "typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^6.11.11",
    "@nestjs/graphql": "^7.3.4",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/typeorm": "^6.3.4",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "eslint-plugin-nestjs": "^1.2.3",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "helmet": "^3.22.0",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@types/express": "^4.17.3",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.30",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "compression": "^1.7.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.17.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.7.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
